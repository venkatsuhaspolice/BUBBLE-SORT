#include<iostream>
using namespace std;
int main(){
    int a[6] = {5,-4,0,-88,43,23};
    int n=6;
    for(int i=0;i<n-1;i++){
        for(int j=0;j<n-1-i;j++){
            if(a[j]>a[j+1]){
                swap(a[j],a[j+1]);
            }
        }
    }
    cout<<"after sorting";
    cout<<endl;
    for(int i=0 ;i<n;i++)
    {
        cout<<a[i]<<" ";
    }
    
    
}
